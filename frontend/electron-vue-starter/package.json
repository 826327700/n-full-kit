{
    "name": "electron-vue-starter",
    "version": "1.0.0",
    "description": "An Electron application with Vue and TypeScript",
    "main": "./out/main/index.js",
    "author": "example.com",
    "homepage": "https://www.electronjs.org",
    "scripts": {
        "format": "prettier --write .",
        "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
        "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
        "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
        "typecheck": "npm run typecheck:node && npm run typecheck:web",
        "preload": "ts-node ./auto-binding.ts",
		"genApi": "swagger-typescript-api -p http://localhost:3000/swagger-json -o ./src/api -n api.ts --axios",
        "start": "electron-vite preview",
        "dev": "npm run preload && electron-vite dev",
        "build": "npm run typecheck && npm run preload && electron-vite build",
        "postinstall": "electron-builder install-app-deps",
        "build:win": "npm run build && electron-builder --win --config",
        "build:mac": "npm run build && electron-builder --mac --config",
        "build:linux": "npm run build && electron-builder --linux --config"
    },
    "dependencies": {
        "@doubleshot/nest-electron": "^0.2.5",
        "@electron-toolkit/preload": "^2.0.0",
        "@electron-toolkit/utils": "^2.0.0",
        "@nestjs/common": "^10.2.8",
        "@nestjs/core": "^10.2.8",
        "@nestjs/microservices": "^10.3.0",
        "@nestjs/platform-express": "^10.2.8",
        "@swc/core": "^1.3.96",
        "axios": "^1.7.9",
        "electron-log": "^5.0.3",
        "electron-updater": "^6.1.1",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@electron-toolkit/eslint-config": "^1.0.1",
        "@electron-toolkit/eslint-config-ts": "^1.0.0",
        "@electron-toolkit/tsconfig": "^1.0.1",
        "@rushstack/eslint-patch": "^1.3.3",
        "@types/node": "^18.17.5",
        "@vitejs/plugin-vue": "^4.3.1",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/eslint-config-typescript": "^11.0.3",
        "electron": "^33.3.1",
        "electron-builder": "^25.1.8",
        "electron-vite": "^2.3.0",
        "eslint": "^8.47.0",
        "eslint-plugin-vue": "^9.17.0",
        "less": "^4.2.0",
        "prettier": "^3.0.2",
        "sass": "^1.69.5",
        "ts-morph": "^25.0.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.1.6",
        "vite": "^4.4.9",
        "vue": "^3.3.4",
        "vue-tsc": "^1.8.8",
		"swagger-typescript-api": "13.0.22"
    }
}
