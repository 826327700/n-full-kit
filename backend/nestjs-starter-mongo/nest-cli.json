{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "builder": "swc"
  },
  "projects": {
    "consul": {
      "type": "library",
      "root": "libs/consul",
      "entryFile": "index",
      "sourceRoot": "libs/consul/src",
      "compilerOptions": {
        "tsConfigPath": "libs/consul/tsconfig.lib.json"
      }
    }
  }
}
